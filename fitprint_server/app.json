{
  "expo": {
    "name": "fitprint",
    "slug": "fitprint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fitprint",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitprint.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.86637725207-5bdas98jrbfr1t3uedqqhorqct169cvj"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fitprint.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "googleAndroidClientId": "86637725207-vtnnpd4t7kr5qel6e475e97mhrapnst5.apps.googleusercontent.com",
      "googleIosClientId": "86637725207-5bdas98jrbfr1t3uedqqhorqct169cvj.apps.googleusercontent.com",
      "googleWebClientId": "86637725207-7tpmbmb1oguvp1h9b7r16r1n6roivlpp.apps.googleusercontent.com",
      "backendBaseUrl": "http://localhost:8081",
      "expoAuthRedirect": "https://auth.expo.io/@rainsuds/fitprint"
    }
  }
}
